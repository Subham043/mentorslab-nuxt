    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@zoomus/websdk": "^2.8.0",
    "chart.js": "2.9.4",
    "core-js": "^3.25.3",
    "element-ui": "^2.15.10",
    "nuxt": "^2.16.3",
    "nuxt-helmet": "^2.1.1",
    "v-viewer": "^1.6.4",
    "vee-validate": "^3.4.14",
    "vue": "^2.7.10",
    "vue-awesome-countdown": "^1.1.4",
    "vue-chartkick": "^0.6.1",
    "vue-fullscreen": "^2.6.1",
    "vue-html-secure": "^1.0.10",
    "vue-pagination-2": "^3.1.0",
    "vue-scrollto": "^2.20.0",
